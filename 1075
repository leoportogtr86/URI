package uri1075;

import java.util.Scanner;


public class Uri1075 {

    
    public static void main(String[] args) {
        
        Scanner sc = new Scanner (System.in);
        
        int n, i;
        
        System.out.println("Digite um valor n:");
        n = sc.nextInt();
        
        for (i = 1; i < 10000; i++){
        
           if (i % n == 2){
              
               System.out.println(i);
           
           }
        
        
        }
        
    }
    
}
