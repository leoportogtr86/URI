package uri1143;

import java.util.Scanner;


public class Uri1143 {

    
    public static void main(String[] args) {
        
        int n, x = 1, i;
        
        Scanner sc = new Scanner (System.in);
        
        System.out.println("Digite o n√∫mero de linhas:");
        n = sc.nextInt();
        
        for (i = 0; i < n; i++){
        
            System.out.printf("%d %d %d %n", x, x*x, x*x*x);
            x++;
        
        }
        
        
    }
    
}
